def swap_case(instring):
    newstring = ''
    for a in instring:
        if a.isupper():
            newstring += (a.lower())
        elif a.islower():
            newstring += (a.upper())
        else:
            newstring += a
    return newstring


if __name__ != '__main__':
    pass
else:
    inString = input()
    result = swap_case(inString)
    print(result)
